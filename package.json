{
  "name": "@statebacked/github-visualizer-bot",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub visualizer bot",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --strict --declaration --module commonjs --outDir dist",
    "prettier": "prettier --write \"src/**/*.ts\" \"scripts/**/*.js\"",
    "lint": "eslint src",
    "smply": "smply",
    "create-machine": "smply machines create --machine github-config --node ./src/index.ts",
    "publish-machine-version": "node ./scripts/publish-version.js github-config",
    "typegen": "xstate typegen ./src/machine.ts"
  },
  "author": "Adam Berger <adam@statebacked.dev>",
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@xstate/cli": "^0.5.2",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "smply": "^0.1.12",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@statebacked/machine-def": "^0.4.1",
    "@statebacked/react-statechart": "^0.1.2",
    "@xstate/machine-extractor": "^0.11.0",
    "octokit": "^3.1.0",
    "react": "^18.2.0",
    "xstate": "^4.38.2"
  }
}
